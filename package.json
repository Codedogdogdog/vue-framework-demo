{
  "name": "vue-framework-demo",
  "version": "1.0.0",
  "description": "A framework demo to update the latest babel and webpack version ",
  "author": "lin <719206525@qq.com>",
  "private": true,
  "scripts": {
    "dev": "webpack serve --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "build": "node build/build.js"
  },
  "sideEffects": false,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue,scss}": [
      "eslint \"*.{js, vue}\" --ignore-pattern \"*.scss\" --fix",
      "stylelint \"*.{css, scss, vue}\" --fix",
      "git add"
    ]
  },
  "dependencies": {
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-regenerator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.0.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.0.0",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.3.0",
    "css-loader": "^5.0.0",
    "enhanced-resolve": "^5.3.1",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^7.1.0",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^5.0.0-alpha.10",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "mini-css-extract-plugin": "^1.2.1",
    "node-notifier": "^8.0.0",
    "portfinder": "^1.0.13",
    "postcss": "^8.1.4",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^4.0.4",
    "postcss-url": "^10.0.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "url-loader": "^4.0.0",
    "vue-eslint-parser": "^7.1.1",
    "vue-loader": "^15.8.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^5.4.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^5.3.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
