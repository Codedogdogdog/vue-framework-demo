{
  "name": "vue-framework-demo",
  "version": "1.0.0",
  "description": "A framework demo to update the latest babel and webpack version ",
  "author": "lin <719206525@qq.com>",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "build": "node build/build.js",
    "create": "Engtools -d ./template"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,vue,scss}": [
        "prettier --print-width 120 --single-quote --trailing-comma all --jsx-bracket-same-line --write",
        "stylelint \"*.scss .vue\" --fix",
        "git add"
      ]
    },
    "ignore": [
      "static/**/*.*"
    ]
  },
  "dependencies": {
    "element-ui": "^2.13.0",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "autoprefixer": "^7.1.2",
    "babel-loader": "^8.0.0",
    "chalk": "^2.0.1",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.0",
    "engineering-tools": "^1.0.10",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "lint-staged": "^8.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-notifier": "^5.1.2",
    "node-sass": "^4.13.0",
    "ora": "^1.2.0",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.2.1",
    "rimraf": "^2.6.0",
    "sass-loader": "^8.0.0",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "stylelint": "^12.0.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^3.1.1",
    "url-loader": "^2.3.0",
    "vue-loader": "^15.8.3",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^4.41.4",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.5.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
